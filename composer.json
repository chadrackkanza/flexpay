{
    "name": "chadrackkanza/flexpay",
    "description": "Package FlexPay API",
    "keywords": [
        "Chadrack KANZA",
        "laravel",
        "flexpay"
    ],
    "homepage": "https://github.com/chadrackkanza/flexpay",
    "license": "MIT",
    "authors": [
        {
            "name": "Chadrack KANZA",
            "email": "chadrackkanza2015@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.14.0",
        "illuminate/contracts": "^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "chadrackkanza\\Flexpay\\": "src/"
        }
    },

    "extra": {
        "laravel": {
            "providers": [
                "chadrackkanza\\Flexpay\\FlexpayServiceProvider"
            ],
            "aliases": {
                "Flexpay": "chadrackkanza\\Flexpay\\Facades\\Flexpay"
            }
        }
    },
    "minimum-stability": "dev"
}
